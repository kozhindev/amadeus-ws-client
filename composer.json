{
  "name": "kozhindev/amadeus-ws-client",
  "type": "library",
  "description": "Fork of 'amabnl/amadeus-ws-client'. SOAP Web Service client library for interacting with the Amadeus GDS through its SOAP interface",
  "keywords": ["amadeus", "gds", "service", "webservice", "soap", "wsdl", "client", "xml", "travel", "airline", "hotel", "booking", "car", "rental", "offer", "masterpricer", "cruise"],
  "minimum-stability": "stable",
  "homepage": "https://github.com/kozhindev/amadeus-ws-client",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Dieter Devlieghere",
      "email": "dieter.devlieghere@gmail.com",
      "homepage": "http://dermika.be",
      "role": "Lead Developer"
    },
    {
      "name": "Aleksandr Bogatikov",
      "email": "perlexed@gmail.com",
      "homepage": "http://www.kozhindev.ru/",
      "role": "Lead Developer"
    }
  ],
  "require": {
    "php": ">=5.4",
    "ext-soap": "*",
    "ext-dom": "*",
    "ext-xsl": "*",
    "psr/log": "^1.0"
  },
  "require-dev": {
    "symfony/yaml": "2.8",
    "phpdocumentor/reflection-docblock": "2.0.*",
    "phpunit/phpunit": "4.8.*"
  },
  "autoload": {
    "psr-4": {
      "Amadeus\\": "src/Amadeus"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\Amadeus\\": "tests/Amadeus"
    }
  }
}